{
    "$schema": "./node_modules/oxlint/configuration_schema.json",
    "env": {
      "builtin": true
    },
    "ignorePatterns": [
      "**/.turbo/**",
      "**/.vercel/**",
      "**/node_modules/**",
      "**/routeTree.gen.ts"
    ],
    "rules": {
      "typescript/no-misused-spread": "off",
      "typescript/unbound-method": "off"
    }
  }