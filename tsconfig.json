{
  "include": ["**/*.ts", "**/*.tsx"],
  "compilerOptions": {
    "lib": ["es2023", "dom"],
    "module": "preserve",
    "target": "es2023",
    "jsx": "preserve",
    "strict": true,
    "skipLibCheck": true,
    "libReplacement": false,
    "erasableSyntaxOnly": true,
    "paths": {
      "~/*": ["./app/*"]
    },
    "types": ["vinxi/types/client"]
  }
}
